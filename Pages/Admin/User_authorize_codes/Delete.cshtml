@page
@model MyOrangeCMS_RazorVersion.Pages.Admin.User_authorize_codes.DeleteModel

@{
    Layout = "_LayoutAdmin"; //页面模板
    ViewData["Title"] = "删除用户";
}

<div class="container">
    <div class="page-header">
        <div class="page-title">
          
            <h3>您确定要删除吗?</h3>
           
        </div>

    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="widget box">
                <div class="widget-header">
                    <h4>
                        <i class="icon-reorder">
                        </i>
                        详细内容
                    </h4>
                    <div class="toolbar no-padding">
                    </div>
                </div>
                <div class="widget-content form-horizontal row-border">
                    <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User_authorize_code.Id)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User_authorize_code.Id)
                         </span>
                       </div>
                      </div>
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User_authorize_code.Nickname)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User_authorize_code.Nickname)
                         </span>
                       </div>
                      </div>
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User_authorize_code.Openid)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User_authorize_code.Openid)
                         </span>
                       </div>
                      </div>
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User_authorize_code.Mobile)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User_authorize_code.Mobile)
                         </span>
                       </div>
                      </div>
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User_authorize_code.Email)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User_authorize_code.Email)
                         </span>
                       </div>
                      </div>
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User_authorize_code.Code)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User_authorize_code.Code)
                         </span>
                       </div>
                      </div>
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User_authorize_code.Sendtime)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User_authorize_code.Sendtime)
                         </span>
                       </div>
                      </div>
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User_authorize_code.Sendstate)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @(Model.User_authorize_code?.Sendstate==1?"成功":"")
                         </span>
                       </div>
                      </div>
 
                      
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User_authorize_code.Messagetype)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User_authorize_code.Messagetype)
                         </span>
                       </div>
                      </div>     
                  
                    <div class="form-group">
                        <label class="col-md-2 ">
                            &nbsp;
                        </label>
                        <div class="col-md-2">
                            <form method="post">
                                <input type="hidden" asp-for="User_authorize_code.Id" />
                                <input type="submit" value="删除" class="btn btn-danger" /> 
                            </form>
                        </div>
                        <div class="col-md-8 text-center">
                            <a asp-page="./Index" class="text-success">返回列表</a>

                        </div>

                    </div>

 




                </div>
            </div>
        </div>
    </div>



</div>


 