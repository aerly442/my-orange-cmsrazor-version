@page
@model MyOrangeCMS_RazorVersion.Pages.Admin.Users.DetailModel

@{
    Layout            = "_LayoutAdmin"; //页面模板
    ViewData["Title"] = "详细";
}



<div class="container">
    <div class="page-header">
        <div class="page-title">
            <h3>
                管理员详细数据
            </h3>
            <span>
                <a asp-page="Index"> <<返回列表</a>
            </span>
        </div>

    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="widget box">
                <div class="widget-header">
                    <h4>
                        <i class="icon-reorder">
                        </i>
                        详细内容
                    </h4>
                    <div class="toolbar no-padding">
                    </div>
                </div>
                <div class="widget-content form-horizontal row-border">

                    <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User.Id)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User.Id)
                         </span>
                       </div>
                      </div>
                                            
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User.Name)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User.Name)
                         </span>
                       </div>
                      </div>
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User.Username)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User.Username)
                         </span>
                       </div>
                      </div>
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User.Email)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User.Email)
                         </span>
                       </div>
                      </div>
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User.Mobile)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User.Mobile)
                         </span>
                       </div>
                      </div>
 
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User.Nickname)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User.Nickname)
                         </span>
                       </div>
                      </div>
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User.Headerpic)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User.Headerpic)
                         </span>
                       </div>
                      </div>
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User.Sex)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User.Sex)
                         </span>
                       </div>
                      </div>
 
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User.Openid)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User.Openid)
                         </span>
                       </div>
                      </div>
                     
 
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User.Intro)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User.Intro)
                         </span>
                       </div>
                      </div>
 
 
 
 
                      <div class="form-group">
                        <label class="col-md-2 ">

                              @Html.DisplayNameFor(model =>model.User.State)
                              
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                              
                               @(this.Model.User.State == 1 ? "正常" : "屏蔽")
                         </span>
                       </div>
                      </div>
 
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User.Lastlogindatetime)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User.Lastlogindatetime)
                         </span>
                       </div>
                      </div>
 
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User.Createtime)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User.Createtime)
                         </span>
                       </div>
                      </div>
 
 
 
 
 
 
                      <div class="form-group">
                        <label class="col-md-2 ">
                              @Html.DisplayNameFor(model => model.User.Updatetime)
                        </label>
                        <div class="col-md-10">
                         <span class="text-default">
                               @Html.DisplayFor(model =>model.User.Updatetime)
                         </span>
                       </div>
                      </div>

 
  
                
                    <div class="form-group">
                        <label class="col-md-2 ">
                            &nbsp;
                        </label>
                        <div class="col-md-2">
                            <a asp-page="./Edit" asp-route-id="@Model.User?.Id" class="btn btn-success">编辑</a> 
                             
                        </div>
                        <div class="col-md-8 text-center">
                            <a asp-page="./Delete" asp-route-id="@Model.User?.Id" class="btn btn-danger">删除</a> 

                        </div>

                    </div>

   
   




                </div>
            </div>
        </div>
    </div>
  
 

</div>